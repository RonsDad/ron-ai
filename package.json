{
  "name": "nira",
  "version": "1.0.0",
  "description": "Nira - Browser automation project with React frontend and FastAPI backend",
  "scripts": {
    "dev": "./scripts/start-all.sh",
    "start": "./scripts/start-all.sh",
    "start:safe": "./scripts/start-safe.sh",
    "frontend": "node server.js",
    "build": "next build",
    "build-start": "next start",
    "backend": "./scripts/start-backend.sh",
    "claude-backend": "./scripts/start-claude.sh",
    "dev:frontend": "node server.js",
    "dev:backend": "uvicorn server.browser_server:app --reload --port 8000",
    "dev:claude": "uvicorn server.claude_backend:app --reload --port 8001",
    "dev:all": "concurrently --kill-others-on-fail \"npm run dev:backend\" \"npm run dev:claude\" \"npm run dev:frontend\"",
    "kill-ports": "node scripts/kill-ports.js",
    "install-all": "npm install"
  },
